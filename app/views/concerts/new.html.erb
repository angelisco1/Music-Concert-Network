<h1>New concert</h1>

<%= form_for @concert do |f| %>
<%= f.label :band %>
<%= f.text_field :band %>
<br>
<%= f.label :venue %>
<%= f.text_field :venue %>
<br>
<%= f.label :city %>
<%= f.text_field :city %>
<br>
<%= f.label :date %>
<%= f.date_field :date %>
<br>
<%= f.label :price %>
<%= f.number_field :price %>
<br>
<%= f.label :description %>
<%= f.text_field :description %>
<br>
<%= f.submit 'Create' %>
<%= link_to("Cancel", concerts_path) %>
<% end %>